{
  "name": "thomasmarinissen/laravel-vercel",
  "description": "Integration to run Laravel on Vercel",
  "keywords": [
    "laravel",
    "vercel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Marinissen"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/console": "^7.0|^8.0|^8.1",
    "illuminate/container": "^7.0|^8.0",
    "illuminate/encryption": "^7.0|^8.0",
    "illuminate/support": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "ThomasMarinissen\\LaravelVercel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ThomasMarinissen\\LaravelVercel\\VercelServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
